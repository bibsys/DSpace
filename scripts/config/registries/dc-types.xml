<dspace-dc-types>
    <!-- HEADER ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
    <dspace-header>
        <title>Augmented Dublin Core field for UCLouvain utility</title>
        <contributor.author>Renaud Michotte</contributor.author>
        <contributor.editor>MickaÃ«l Pourbaix</contributor.editor>
    </dspace-header>
    <!-- SCHEMA ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
    <dc-schema>
        <name>dc</name>
        <namespace>http://dublincore.org/documents/dcmi-terms/</namespace>
    </dc-schema>
    <!-- FIELDS ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
    <dc-type>
        <schema>dc</schema>
        <element>language</element>
        <qualifier>iso-639-2</qualifier>
        <scope_note>ISO 639-2 code language</scope_note>
    </dc-type>
    <dc-type>
        <schema>dc</schema>
        <element>description</element>
        <qualifier>diffusion</qualifier>
        <scope_note>Allow to specify a statement about the publication diffusion.</scope_note>
    </dc-type>
    <dc-type>
        <schema>dc</schema>
        <element>description</element>
        <qualifier>tag</qualifier>
        <scope_note>Custom tag placed on a publication [ie: accepted, submitted, ...]</scope_note>
    </dc-type>
    <dc-type>
        <schema>dc</schema>
        <element>identifier</element>
        <qualifier>fedora</qualifier>
        <scope_note>To store original FedoraCommons PID</scope_note>
    </dc-type>
 </dspace-dc-types>